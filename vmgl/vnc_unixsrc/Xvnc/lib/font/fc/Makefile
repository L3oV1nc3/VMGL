         HEADERS =
            SRCS = fsconvert.c fserve.c fsio.c transport.c
            OBJS = fsconvert.o fserve.o fsio.o transport.o
         DEFINES = $(SIGNAL_DEFINES)
    CONN_DEFINES = -DUNIXCONN -DTCPCONN -DFONT_t -DTRANS_CLIENT -DTRANS_SERVER -DTRANS_REOPEN

  DEPEND_DEFINES = $(TRANS_INCLUDES) $(CONN_DEFINES)

all:: DONE

DONE: $(OBJS)
	$(RM) $@
	touch $@

clean::
	$(RM) DONE

.c.o:
	$(RM) $@
	 	$(CC) -c $(CFLAGS) $(_NOOP_) $*.c

fsio.o:	fsio.c $(ICONFIGFILES)
	$(RM) $@
	 	$(CC) -c $(CFLAGS) $(CONN_DEFINES) $(SOCK_DEFINES) $*.c

fsio.i: fsio.c $(ICONFIGFILES)
	$(RM) $@
	 	$(CC) -E $(CFLAGS) $(CONN_DEFINES) $(SOCK_DEFINES) fsio.c > $@

transport.o:	transport.c $(ICONFIGFILES)
	$(RM) $@
	 	$(CC) -c $(CFLAGS) $(TRANS_INCLUDES) $(CONN_DEFINES) $(SOCK_DEFINES) $*.c

transport.i: transport.c $(ICONFIGFILES)
	$(RM) $@
	 	$(CC) -E $(CFLAGS) $(TRANS_INCLUDES) $(CONN_DEFINES) $(SOCK_DEFINES) transport.c > $@

transport.c:  $(TRANSCOMMSRC)/transport.c
	$(RM) $@
	$(LN) $? $@

includes:: transport.c

depend:: transport.c

clean::
	$(RM) transport.c

lint:
	$(LINT) $(LINTFLAGS) $(SRCS) $(LINTLIBS)
lint1:
	$(LINT) $(LINTFLAGS) $(FILE) $(LINTLIBS)

depend:: $(DEPEND)

$(DEPEND):
	@echo "checking $@ over in $(DEPENDSRC) first..."; \
	cd $(DEPENDSRC) && $(MAKE); \
	echo "okay, continuing in $(CURRENT_DIR)"

depend::
	$(DEPEND) $(DEPENDFLAGS) -- $(ALLDEFINES) $(DEPEND_DEFINES) -- $(SRCS)

